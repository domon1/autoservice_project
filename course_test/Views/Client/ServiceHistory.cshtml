@model IEnumerable<DataLayer.Entityes.Order>
<h1>Service History</h1>
<div>
    <table class="table">
        <tr>
            <td>OrderNumber</td>
            <td>Date</td>
            <td>Total Price</td>
            <td>Details</td>
        </tr>
        @foreach (var service in Model)
        {
            <tr>
                <td>@service.OrderId</td>
                <td>@service.Date</td>
                <td></td>
                <td>
                    <a asp-action="MoreOrder" asp-controller="Client" asp-route-id="@service.OrderId">More</a>
                </td>
            </tr>
        }
    </table>
    <a asp-action="Index" asp-controller="CLient">Back</a>
</div>

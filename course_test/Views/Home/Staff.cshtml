@model IEnumerable<DataLayer.Entityes.Staff>
@{
    ViewData["Title"] = "Staff";
}
<h1>All Staff</h1>
<div>
    <table class="table">
        <tr>
            <td>Name</td>
            <td>Surname</td>
            <td>Specialization</td>
            <td></td>
        </tr>
        @foreach (var service in Model)
        {
            <tr>
                <td>@service.Name</td>
                <td>@service.Surname</td>
                <td>@service.Specialization</td>
            </tr>
        }
    </table>
    <input type="button" value="Exit" onclick='window.location = "@Url.Action("Index", "Home")";' />
</div>
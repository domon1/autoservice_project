@model  IEnumerable<DataLayer.Entityes.CarService>
@{

}
<h1>Services</h1>
<div>
    <table class="table">
        <tr>
            <td>Id</td>
            <td>Category</td>
            <td>Name</td>
            <td>Price</td>
            <td>Duration</td>
            <td></td>
            <td></td>
        </tr>
        @foreach (var service in Model)
        {
            <tr>
                <td>@service.CarServiceId</td>
                <td>@service.Category</td>
                <td>@service.Name</td>
                <td>@service.Price</td>
                <td>@service.Duration</td>
                <td><a href="~/Manager/Update/@service.CarServiceId">Update</a></td>
                <td><a href="~/Manager/Delete/@service">Delete</a></td>
            </tr>
        }
        </table>
</div>